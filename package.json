{
  "name": "semantic-release-vsce",
  "version": "0.0.0-development",
  "description": "semantic-release plugin to package and publish VS Code extensions",
  "license": "MIT",
  "engines": {
    "node": ">=10.18"
  },
  "repository": "https://github.com/felipecrs/semantic-release-vsce.git",
  "bugs": "https://github.com/felipecrs/semantic-release-vsce/issues",
  "homepage": "https://github.com/felipecrs/semantic-release-vsce#readme",
  "author": "Morten Henriksen <mh@gi2.dk>",
  "keywords": [
    "semantic-release",
    "semantic-release-plugin",
    "vscode",
    "vsce"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "release": "semantic-release",
    "test": "nyc ava",
    "posttest": "npm run lint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "nyc": {
    "all": true,
    "exclude": [
      "lib/die.js"
    ],
    "include": [
      "index.js",
      "lib/**/*.js"
    ]
  },
  "release": {
    "preset": "conventionalcommits"
  },
  "volta": {
    "node": "14.17.0",
    "npm": "7.12.1"
  },
  "dependencies": {
    "@semantic-release/error": "^2.2.0",
    "detect-indent": "^6.0.0",
    "execa": "^5.0.0",
    "fs-extra": "^9.0.1",
    "read-pkg-up": "^7.0.1",
    "vsce": "^1.85.1"
  },
  "peerDependencies": {
    "semantic-release": ">=16.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "ava": "^3.9.0",
    "conventional-changelog-conventionalcommits": "^4.2.1",
    "eslint": "^7.19.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^5.1.2",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "semantic-release": "^17.0.3",
    "sinon": "^10.0.0"
  }
}
