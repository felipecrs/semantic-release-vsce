{
  "name": "semantic-release-vsce",
  "version": "0.0.0-development",
  "description": "semantic-release plugin to package and publish VS Code extensions",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "repository": "https://github.com/felipecrs/semantic-release-vsce.git",
  "bugs": "https://github.com/felipecrs/semantic-release-vsce/issues",
  "homepage": "https://github.com/felipecrs/semantic-release-vsce#readme",
  "author": "Morten Henriksen <mh@gi2.dk>",
  "contributors": [
    "Felipe Santos <felipecassiors@gmail.com>"
  ],
  "keywords": [
    "semantic-release",
    "semantic-release-plugin",
    "vscode",
    "vsce"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "prettier --check . && eslint .",
    "release": "semantic-release",
    "test": "nyc ava",
    "posttest": "npm run lint",
    "prepare": "husky install"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ]
  },
  "nyc": {
    "all": true,
    "exclude": [
      "lib/die.js"
    ],
    "include": [
      "index.js",
      "lib/**/*.js"
    ]
  },
  "volta": {
    "node": "18.12.1",
    "npm": "9.1.1"
  },
  "dependencies": {
    "@semantic-release/error": "^3.0.0",
    "@vscode/vsce": "^2.15.0",
    "execa": "^7.1.0",
    "fs-extra": "^11.1.0",
    "glob": "^8.0.3",
    "ovsx": "^0.8.0"
  },
  "peerDependencies": {
    "semantic-release": ">=18"
  },
  "devDependencies": {
    "ava": "^5.0.1",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-n": "^15.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "nyc": "^15.1.0",
    "prettier": "2.8.4",
    "proxyquire": "^2.1.3",
    "semantic-release": "^20.0.2",
    "sinon": "^15.0.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
